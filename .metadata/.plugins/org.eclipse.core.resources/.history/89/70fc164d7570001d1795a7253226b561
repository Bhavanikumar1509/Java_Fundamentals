    <property name="price" value="800"/>
    </bean>
    <bean id="book2" class ="com.beans2.Book">
    <bean id="book3" class ="com.beans2.Book">
   	<bean id="libraryList" class ="com.beans2.LibraryList">
   	<property name="books">
   	<list>
   	<ref bean="book1"/>
   	<ref bean="book2"/>
   	<ref bean="book3"/>
   	</list>
   	
   	</property>
</bean>
</bean>
