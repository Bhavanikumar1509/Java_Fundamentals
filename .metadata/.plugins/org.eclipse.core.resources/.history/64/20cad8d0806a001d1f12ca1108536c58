package Test;
import java.util.*;
class Student{
	private Integer admno;
	private String sname;
	private float english,maths,science;
	private float total;
	static {
		String collegename="IIIT_Ongole";
		String Coursename="CSE";
	}
	private float ctotal() {
		
		return english+maths+science;
		
	}
	public void Takedata() {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the admno:");
		admno=sc.nextInt();
		sc.nextLine();
		System.out.print("Enter the sname:");
		sname=sc.nextLine();
		System.out.print("Enter the english marks:");
		english=sc.nextFloat();
		System.out.print("Enter the maths marks:");
		maths=sc.nextFloat();
		System.out.print("Enter the science marks:");
		science=sc.nextFloat();		
	}
	public void showdata(String collegename, String Coursename) {
		System.out.println("admno:"+admno);
		System.out.println("name:"+sname);
		System.out.println("english marks:"+english);
		System.out.println("mahts marks::"+maths);
		System.out.println("science maths:"+science);
		System.out.println("Total:"+ctotal());
		System.out.println("collegename:"+collegename);
		System.out.println("coursename:"+Coursename);
		
		
		
	}
	
}
public class OOP_COncepts {
	public static void main(String[] args) {
		Student st[]=new Student[10];
		for(int i=1;i<=5;i++) {
		st[i].Takedata();
		st[i].showdata();
		}
		
	}

}
